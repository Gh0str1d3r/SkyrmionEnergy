   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"t", "=", "1"}], 
       RowBox[{"(*", " ", 
        RowBox[{"single", " ", "magnetic", " ", "layer", " ", "thickness"}], 
        " ", "*)"}], ",", "\[IndentingNewLine]", 
       RowBox[{"p", "=", "4"}], " ", 
